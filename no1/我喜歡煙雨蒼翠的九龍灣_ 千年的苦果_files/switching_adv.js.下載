// ===[[advertisement end]]===

// ===[[output start]]===
if (window.NIFTY.cocolog.ad.execute) {
    NIFTY.cocolog.ad.func[window.NIFTY.cocolog.ad.arg.ad].open();
    if (NIFTY.cocolog.ad.googleflag) {
        NIFTY.cocolog.ad.func[window.NIFTY.cocolog.ad.arg.ad].afc();
    } else {
        NIFTY.cocolog.ad.func[window.NIFTY.cocolog.ad.arg.ad].not_afc();
    };
    NIFTY.cocolog.ad.func[window.NIFTY.cocolog.ad.arg.ad].close();
}
// ===[[output end]]===

