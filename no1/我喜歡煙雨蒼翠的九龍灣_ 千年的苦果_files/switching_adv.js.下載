        );
    },
    open : function() {
        document.write("<div id='cocolog-fpa-header' style='margin: 7px 0 15px 0 !important;'><div id='cocolog-fpa-header-inner' style='text-align: center;' ><div class='ad_header' style='text-align: center; display: inline-block'>");
    },
    close : function() {
        document.write("</div></div></div>");
    }
}
// ---[header end]---

