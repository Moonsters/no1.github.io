// cocolog: switching advertisement

if (!window.NIFTY) {  window.NIFTY = {}; }
if (!window.NIFTY.cocolog) {  window.NIFTY.cocolog = {}; }
if (!window.NIFTY.cocolog.ad) {  window.NIFTY.cocolog.ad = {}; }

// ===[[argument start]]===
if (!window.NIFTY.cocolog.ad.arg) { window.NIFTY.cocolog.ad.arg = {}; }
window.NIFTY.cocolog.ad.execute = undefined;
(function (){
    var scripts = document.getElementsByTagName( 'script' );
    var src = scripts[ scripts.length - 1 ].src;

    var query = src.substring( src.indexOf( '?' ) + 1 );
    var parameters = query.split( '&' );

    var result = new Object();
    for( var i = 0; i < parameters.length; i++ )
    {
        var element = parameters[ i ].split( '=' );

        var paramName  = decodeURIComponent( element[ 0 ] );
        var paramValue = decodeURIComponent( element[ 1 ] );

        result[ paramName ] = paramValue;
    }

    window.NIFTY.cocolog.ad.arg = result;
})();
// ===[[argument end]]===

// ===[[arg check start]]===
window.NIFTY.cocolog.ad.type = {
    header: 1,
    content: 1,
    middle: 1,
    footer: 1
}
if (window.NIFTY.cocolog.ad.arg.ad && (window.NIFTY.cocolog.ad.arg.ad in window.NIFTY.cocolog.ad.type)) {
    window.NIFTY.cocolog.ad.execute = true;
}
// ===[[arg check end]]===

// ===[[advertisement start]]===
if (!window.NIFTY.cocolog.ad.func) { window.NIFTY.cocolog.ad.func = {}; }

// ---[header start]---
window.NIFTY.cocolog.ad.func['header'] = {
    not_afc : function() {
//GNG HBNR
        document.write(
